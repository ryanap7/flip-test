{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["src/Assets/*"],
      "@components/*": ["src/Components/*"],
      "@hooks/*": ["src/Hooks/*"],
      "@modules/*": ["src/Modules/*"],
      "@navigations/*": ["src/Navigations/*"],
      "@screens/*": ["src/Screens/*"],
      "@stores/*": ["src/Stores/*"],
      "@themes/*": ["src/Themes/*"],
      "@utils/*": ["src/Utils/*"]
    }
  }
}
